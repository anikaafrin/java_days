import java.util.*;
public class Patterns {
    public static int calculateSum(int a, int b){
         
        return a*b;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int product = calculateSum(a, b);
        System.out.print(product);
 
    
        
        
    }
}
